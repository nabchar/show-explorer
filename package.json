{
  "name": "exercise-frontend",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/VICEMedia/exercise-frontend",
  "author": "Matthew Panton <matthew.panton@vice.com>",
  "license": "MIT",
  "scripts": {
    "start-api": "json-server --watch shows.json",
    "client": "webpack-dev-server --config ./webpack/webpack.dev.js",
    "client:prod": "webpack-dev-server --config ./webpack/webpack.config.js",
    "dev": "concurrently --kill-others-on-fail \"npm run start-api\" \"npm run client\"",
    "lint": "eslint client/**/*.jsx client/**/**/*.jsx client/**/*.js client/**/**/*.js webpack/** test/**",
    "lint:fix": "eslint --fix client/**/*.jsx client/**/**/*.jsx client/**/*.js client/**/**/*.js webpack/** test/**",
    "start": "concurrently --kill-others-on-fail \"npm run start-api\" \"npm run client:prod\"",
    "test:unit": "mocha --colors -t 12000 --require ./babel-util.js --require ./test/helpers.js --require ./test/dom.js 'client/**/**/*.spec.js'",
    "test:unit:watch": "npm run test:unit -- --watch"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.6",
    "@fortawesome/free-solid-svg-icons": "^5.4.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "ajv": "^6.5.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "json-server": "0.12.1",
    "node-sass": "^4.9.3",
    "prop-types": "^15.6.2",
    "query-string": "^6.2.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "devDependencies": {
    "async-wait-until": "^1.2.4",
    "concurrently": "^4.0.1",
    "chai": "^4.2.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "fake-fetch": "^2.0.0",
    "fetch-mock": "^7.2.0",
    "jsdom": "^12.2.0",
    "mocha": "^5.2.0",
    "react-test-renderer": "^16.5.2",
    "sinon": "^7.0.0"
  }
}
